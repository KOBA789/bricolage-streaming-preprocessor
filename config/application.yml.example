spring:
    datasource:
        url: jdbc:postgresql://DBHOST:5432/DBNAME
        driver-class-name: org.postgresql.Driver
        username: USER_NAME
        password: PASSWORD
        max-active: 10
        max-wait: 10000
        test-on-borrow: true
        test-while-idle: true
        time-between-eviction-runs-millis: 60000
        validation-query: "select 1"

flyway:
    enabled: false
